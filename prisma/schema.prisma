model Post {
  id        Int      @default(autoincrement()) @id
  title     String
  content   String
}generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Address {
  id        Int      @default(autoincrement()) @id
  street    String
  city      String
  country   String
  user      User?
}
model User {
  id            Int         @default(autoincrement()) @id
  email         String      @unique
  name          String
  password      String
  address       Address?     @relation(fields: [addressId], references: [id])
  addressId     Int? @unique
}